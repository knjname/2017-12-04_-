{
    "compilerOptions": {
        "sourceMap": true,
        "target": "es6",
        "jsx": "react",
        "module": "commonjs",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "strictNullChecks": false,
        "baseUrl": ".",
        "skipLibCheck": true,
        "lib": [
            "es6",
            "es7",
            "dom"
        ]
    },
    "exclude": [
        "node_modules",
        "dist",
        "build",
        "gradle",
        "typedoc",
        ".awcache",
        ".vscode",
        ".gradle"
    ]
}